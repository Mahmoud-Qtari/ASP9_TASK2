@model List<User>

<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Active</th>
        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model)
        {
            <tr>
                <th scope="row">@item.Id</th>
                <td>@item.Name</td>
                <td>
                    <a asp-controller="User" asp-action="ChangeActive" asp-route-id="@item.Id"><button class="btn btn-primary">Disable</button></a>
                </td>
            </tr>

        }
    </tbody>
</table>